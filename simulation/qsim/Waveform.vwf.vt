module Integrador_vlg_vec_tst();
// constants                                           
// general purpose registers
reg Clock;
reg Continuar;
reg [11:0] treg_DATA;
reg [2:0] K;
reg LOAD_F;
reg LOAD_I;
reg Recargar;
reg START;
reg UpDown;
// wires                                               
wire [11:0] A;
wire [11:0] B;
wire [11:0] DATA;
wire ERROR;
wire FIN;
wire PAUSA;

wire sampler;                             

// assign statements (if any)                          
assign DATA = treg_DATA;
Integrador i1 (
// port map - connection between master ports and signals/registers   
	.A(A),
	.B(B),
	.Clock(Clock),
	.Continuar(Continuar),
	.DATA(DATA),
	.ERROR(ERROR),
	.FIN(FIN),
	.K(K),
	.LOAD_F(LOAD_F),
	.LOAD_I(LOAD_I),
	.PAUSA(PAUSA),
	.Recargar(Recargar),
	.START(START),
	.UpDown(UpDown)
);

// Clock
always
begin
	Clock = 1'b0;
	Clock = #5000 1'b1;
	#5000;
end 

// Continuar
initial
begin
	Continuar = 1'b0;
end 
// DATA[ 11 ]
initial
begin
	treg_DATA[11] = 1'b0;
	treg_DATA[11] = #40000 1'bZ;
end 
// DATA[ 10 ]
initial
begin
	treg_DATA[10] = 1'b0;
	treg_DATA[10] = #40000 1'bZ;
end 
// DATA[ 9 ]
initial
begin
	treg_DATA[9] = 1'b0;
	treg_DATA[9] = #40000 1'bZ;
end 
// DATA[ 8 ]
