module Integrador_vlg_sample_tst(
	Clock,
	Continuar,
	DATA,
	K,
	LOAD_F,
	LOAD_I,
	Recargar,
	START,
	UpDown,
	sampler_tx
);
input  Clock;
input  Continuar;
input [11:0] DATA;
input [2:0] K;
input  LOAD_F;
input  LOAD_I;
input  Recargar;
input  START;
input  UpDown;
output sampler_tx;

reg sample;
time current_time;
always @(Clock or Continuar or DATA or K or LOAD_F or LOAD_I or Recargar or START or UpDown)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

