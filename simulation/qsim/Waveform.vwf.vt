begin
	treg_DATA[8] = 1'b0;
	treg_DATA[8] = #40000 1'bZ;
end 
// DATA[ 7 ]
initial
begin
	treg_DATA[7] = 1'b0;
	treg_DATA[7] = #40000 1'bZ;
end 
// DATA[ 6 ]
initial
begin
	treg_DATA[6] = 1'b0;
	treg_DATA[6] = #20000 1'b1;
	treg_DATA[6] = #20000 1'bZ;
end 
// DATA[ 5 ]
initial
begin
	treg_DATA[5] = 1'b1;
	treg_DATA[5] = #20000 1'b0;
	treg_DATA[5] = #20000 1'bZ;
end 
// DATA[ 4 ]
initial
begin
	treg_DATA[4] = 1'b1;
	treg_DATA[4] = #20000 1'b0;
	treg_DATA[4] = #20000 1'bZ;
end 
// DATA[ 3 ]
initial
begin
	treg_DATA[3] = 1'b1;
	treg_DATA[3] = #40000 1'bZ;
end 
// DATA[ 2 ]
initial
begin
	treg_DATA[2] = 1'b1;
	treg_DATA[2] = #20000 1'b0;
	treg_DATA[2] = #20000 1'bZ;
end 
// DATA[ 1 ]
initial
begin
	treg_DATA[1] = 1'b1;
	treg_DATA[1] = #20000 1'b0;
	treg_DATA[1] = #20000 1'bZ;
end 
// DATA[ 0 ]
initial
begin
	treg_DATA[0] = 1'b1;
	treg_DATA[0] = #20000 1'b0;
	treg_DATA[0] = #20000 1'bZ;
end 
// K[ 2 ]
initial
begin
	K[2] = 1'b1;
end 
// K[ 1 ]
initial
begin
	K[1] = 1'b0;
end 
// K[ 0 ]
initial
begin
	K[0] = 1'b1;
end 

// LOAD_F
initial
begin
	LOAD_F = 1'b0;
	LOAD_F = #20000 1'b1;
	LOAD_F = #20000 1'b0;
end 

// LOAD_I
initial
begin
	LOAD_I = 1'b1;
	LOAD_I = #20000 1'b0;
end 

// Recargar
initial
begin
	Recargar = 1'b1;
	Recargar = #40000 1'b0;
end 

// START
initial
begin
	START = 1'b0;
	START = #40000 1'b1;
end 

// UpDown
initial
begin
	UpDown = 1'b1;
end 

Integrador_vlg_sample_tst tb_sample (
	.Clock(Clock),
	.Continuar(Continuar),
	.DATA(DATA),
	.K(K),
	.LOAD_F(LOAD_F),
	.LOAD_I(LOAD_I),
	.Recargar(Recargar),
	.START(START),
	.UpDown(UpDown),
	.sampler_tx(sampler)
);

Integrador_vlg_check_tst tb_out(
	.A(A),
	.B(B),
	.DATA(DATA),
	.ERROR(ERROR),
	.FIN(FIN),
	.PAUSA(PAUSA),
	.sampler_rx(sampler)
);
endmodule

